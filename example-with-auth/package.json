    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.4",
    "@eslint/js": "9.39.3",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.56.1",
    "@typescript-eslint/parser": "8.56.1",
    "@vitejs/plugin-react": "4.7.0",
    "eslint": "9.39.3",
    "globals": "16.5.0",
    "npm-run-all2": "8.0.4",
    "typescript": "5.9.3",
    "vite": "6.4.1"
